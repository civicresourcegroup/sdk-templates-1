{
  "documentation": {
    "title": "Augmented Reality",
    "copyright": "CopyrightÂ© CivicConnect, 2018"
  },
  "constants": {
    "textPosition": {
      "justifyContent": "center",
      "alignItems": "center",
      "height": 40,
      "backgroundColor": "brown",
      "padding": 10,
      "marginTop": 10,
      "marginLeft": 25,
      "marginRight": 25,
      "borderRadius": 5
    },
    "textStyle": {
      "fontSize": 14,
      "textAlign": "center",
      "marginLeft": 10,
      "paddingLeft": 2,
      "color": "white"
    }
  },
  "render": [
    {
      "row": "+1",
      "type": "renderText",
      "style": ".constants.textPosition",
      "properties": {
        "text": "Street View",
        "style": ".constants.textStyle",
        "action": [
          {
            "action": "home.ShowStreetView",
            "title": "Willie Mays Plaza",
            "showLiveCamera": false,
            "navBarHidden": false,
            "hasNavBar": true,
            "photosphereParams": {
              "region": {
                "address": "Willie Mays Plaza, San Francisco, CA, USA"
              }
            },
            "fetcherRefs": [
              "wifi",
              "congestion",
              "twitter"
            ]
          }
        ]
      }
    },
    {
      "row": "+1",
      "type": "renderText",
      "style": ".constants.textPosition",
      "properties": {
        "text": "Live Camera View",
        "style": ".constants.textStyle",
        "action": [
          {
            "action": "home.ShowStreetView",
            "title": "Live Camera",
            "showLiveCamera": true,
            "navBarHidden": false,
            "hasNavBar": true,
            "fetcherRefs": [
              "wifi",
              "congestion",
              "twitter"
            ]
          }
        ]
      }
    }
  ]
}